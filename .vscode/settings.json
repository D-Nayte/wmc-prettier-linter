{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },

  "eslint.experimental.useFlatConfig": true,
  "eslint.rules.customizations": [{ "rule": "no-console", "severity": "off" }]
}
